<h1> All Mutation Details </h1>

<table>
    <thead>
        <tr>
            <th>Mutation</th>
            <th>Isoform</th>
            <th>Domain</th>
            <th>Actions</th>
        </tr>
    </thead>
<tbody>
    <% @mutation.each do |mutation| %>
        <tr>
            <td><%= mutation.mutation %></td>
            <td><%= mutation.isoform %></td>
            <td><%= mutation.domain %></td>
            <td><%= link_to 'Show', mutation_path(mutation) %></td>
        </tr>
    <% end %>
</tbody>
</table>